{% block title %}Tyres{% endblock %}

{% block body %}
    <h1>Tyres</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ tyre.id }}</td>
            </tr>
            <tr>
                <th>Width</th>
                <td>{{ tyre.width }}</td>
            </tr>
            <tr>
                <th>Diameter</th>
